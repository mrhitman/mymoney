{
  "name": "mymoney-api-v2",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "npm run build && node dist/server.js",
    "start:dev": "ts-node-dev --cls --watch -- src/server.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "migrate": "knex migrate:latest --cwd src --knexfile knexfile.ts",
    "migrate:down": "knex migrate:rollback --cwd src --knexfile knexfile.ts",
    "migrate:make": "knex migrate:make --cwd src --knexfile knexfile.ts -x ts",
    "seed": "knex seed:run --cwd src --knexfile knexfile.ts",
    "seed:make": "knex seed:make --cwd src --knexfile knexfile.ts -x ts"
  },
  "dependencies": {
    "async-redis": "^1.1.7",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "dotenv": "^8.2.0",
    "fixer-api": "^2.0.2",
    "joi": "^14.3.1",
    "knex": "^0.20.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-cors": "^0.0.16",
    "koa-helmet": "^5.2.0",
    "koa-jwt": "^3.6.0",
    "koa-morgan": "^1.0.1",
    "koa-passport": "^4.1.3",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.15",
    "objection": "^1.6.11",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "rimraf": "^3.0.0",
    "supertest-koa-agent": "^0.3.2",
    "ts-httpexceptions": "^4.1.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.2",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "^24.0.23",
    "@types/joi": "^14.3.4",
    "@types/knex": "^0.16.1",
    "@types/koa": "^2.0.52",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-cors": "^0.0.0",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-morgan": "^1.0.4",
    "@types/koa-passport": "^4.0.2",
    "@types/lodash": "^4.14.146",
    "@types/node": "^12.12.7",
    "@types/passport-jwt": "^3.0.3",
    "@types/xml2js": "^0.4.5",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.0",
    "tslint": "^5.20.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
