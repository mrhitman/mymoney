{
  "name": "api",
  "version": "0.1.1",
  "description": "",
  "author": "mrhitman",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "reset:db": "pnpm run migrate:down && pnpm run migrate && pnpm run seed",
    "migrate": "knex migrate:latest --cwd src --knexfile knexfile.ts",
    "migrate:prod": "knex migrate:latest --cwd dist --knexfile knexfile.js",
    "migrate:down": "knex migrate:rollback --cwd src --knexfile knexfile.ts",
    "migrate:make": "knex migrate:make --cwd src --knexfile knexfile.ts -x ts",
    "seed": "knex seed:run --cwd src --knexfile knexfile.ts",
    "seed:prod": "knex seed:run --cwd dist --knexfile knexfile.js",
    "seed:make": "knex seed:make --cwd src --knexfile knexfile.ts -x ts"
  },
  "dependencies": {
    "@apollo/gateway": "^0.16.6",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.4.1",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/serve-static": "^2.1.3",
    "apollo-server-express": "^2.14.4",
    "apollo-server-plugin-base": "^0.10.2",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^3.2.1",
    "cache-manager-redis-store": "^2.0.0",
    "chance": "^1.1.5",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "fixer-api": "^2.1.1",
    "graphql": "^15.1.0",
    "graphql-query-complexity": "^0.6.0",
    "graphql-tools": "^6.0.9",
    "graphql-type-json": "^0.3.2",
    "jest": "25.5.4",
    "knex": "^0.20.15",
    "lodash": "^4.17.15",
    "luxon": "^1.24.1",
    "moment": "2.29.1",
    "nestjs-redis": "^1.2.7",
    "objection": "^2.1.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "s": "^1.0.0",
    "typescript": "3.9.7",
    "uuid": "^8.0.0",
    "xml2js": "^0.4.23",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.8",
    "@types/bcrypt": "^3.0.0",
    "@types/chance": "^1.1.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.3",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "25.2.1",
    "@types/lodash": "^4.14.151",
    "@types/luxon": "^1.24.0",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.3",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "25.5.4",
    "prettier": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "25.3.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "./"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "jest": "25.5.4"
  }
}
