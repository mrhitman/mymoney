{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es2015", "dom"],
    "target": "es2018",
    "sourceMap": true,
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "models": ["src/models"]
    },
    "outDir": "dist",
    "incremental": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": ["node_modules", "dist", "src/knexfile.ts"]
}
