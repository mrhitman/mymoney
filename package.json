  "private": true,
  "scripts": {
    "start:api": "yarn start:api:services && yarn start:dev",
    "stop:api:services": "cd api && docker-compose down",
    "start:api:services": "cd api && docker-compose up -d",
    "start:dashboard": "cd dashboard && yarn start",
    "start": "concurrently \"yarn start:api\" \"yarn start:dashboard\""
  },
  "workspaces": {
    "nohoist": "**/*",
    "packages": [
      "api",
      "dashboard",
      "common"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0"
  }
}
